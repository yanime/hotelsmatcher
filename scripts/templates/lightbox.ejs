<%
if( obj.id !== undefined ) {
%>
<ul class="hotel info column">
    <li class="header">
        <a href="#hotel/<%=obj.hotelId %>"><%= obj.name %></a>
    </li>
    <li class="radio trips pin">
        <input type="hidden" name="pinned" value="<% obj.pinned %>" />
        <span class="label">hotel pinned</span>
        <div class="check"></div>
    </li>
    <li class="photos">
        <button class="previous"></button>
        <div class="images">
            <img src="http://lorempixel.com/158/82/city" />
        </div>
        <button class="next"></button>
    </li>
    <li class="price"><span>Price </span><span class="currency"> $ </span><span class="value">345</span></li>
    <li class="book"><a href="<%=obj.deepLink %>" target="_blank" class="button action book" title="Book for this hotel">Book now</a></li>
</ul>
<div class="close"></div>
<%
}
%>
