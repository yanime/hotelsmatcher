    <li class="airport-shuttle option checkbox unchecked <%=(obj.facilities.airport_shuttle == true)? ' active' : '' %>">
        <input type="hidden" id="airport_shuttle" value="<%=(obj.facilities.airport_shuttle == true)%>" /><div class="check"></div><span>Airport Shuttle</span></li>
    <li class="fitness-center option checkbox unchecked<%=(obj.facilities.fitness_center == true)? ' active' : '' %>">
        <input type="hidden" id="fitness_center" value="<%=(obj.facilities.fitness_center == true)%>" /><div class="check"></div><span>Fitness Center</span></li>
    <li class="check-more facilities">check more (5) ...</li>
