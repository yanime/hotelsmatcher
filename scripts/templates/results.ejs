        <li class="pool option checkbox unchecked <%=(obj.facilities.pool == true) ? ' active' : '' %>">
            <input type="hidden" id="pool" value="<%=(obj.facilities.pool == true)%>" /><div class="check"></div><span>Pool</span></li>
        <li class="free_parking option checkbox unchecked <%=(obj.facilities.free_parking == true) ? ' active' : '' %>">
            <input type="hidden" id="free_parking" value="<%=(obj.facilities.free_parking == true)%>" /><div class="check"></div><span>Free Parking</span></li>
        <li class="kitchen option checkbox unchecked <%=(obj.facilities.kitchen == true) ? ' active' : '' %>">
            <input type="hidden" id="kitchen" value="<%=(obj.facilities.kitchen == true)%>" /><div class="check"></div><span>Kitchen</span></li>
        <li class="pets_allowed option checkbox unchecked <%=(obj.facilities.pets_allowed == true) ? ' active' : '' %>">
