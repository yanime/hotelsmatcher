<ul class="headers column">
    <li class="header">
        Hotels
    </li>
    <li class="pin"></li>
    <li class="price"><span>Price</span></li>
    <li class="stars"><span>Stars</span></li>
    <li class="tripadvisor-reviews"><span>TripAdvisor Reviews</span></li>
    <li class="distance"><span>Distance from area</span></li>
    <li class="photos"><span>Photos</span></li>
    <li class="facilities category title"><span>Facilities</span></li>
    <% if((obj.facilities.wifi == true)){%> <li class="wifi option checkbox <%=(obj.facilities.wifi == true)? 'checked' : 'unchecked' %>"><div class="check"></div><span>Wifi</span></li><%}%>
    <% if((obj.facilities.parking == true)){%>  <li class="parking option checkbox <%=(obj.facilities.parking == true)? 'checked' : 'unchecked' %>"><div class="check"></div><span>Parking</span></li><%}%>
    <% if((obj.facilities.airport_shuttle == true)){%> <li class="airport-shuttle option checkbox <%=(obj.facilities.airport_shuttle == true)? 'checked' : 'unchecked' %>"><div class="check"></div><span>Airport Shuttle</span></li><%}%>
    <% if((obj.facilities.fitness_center == true)){%> <li class="fitness-center option checkbox <%=(obj.facilities.fitness_center == true)? 'checked' : 'unchecked' %>"><div class="check"></div><span>Fitness Center</span></li><%}%>
    <% if((obj.facilities.spa == true)){%> <li class="spa option checkbox <%=(obj.facilities.spa == true) ? 'checked' : 'unchecked' %>"><div class="check"></div><span>Spa</span></li><%}%>
    <li class="see-all facilities"></li>
    <li></li>
</ul>
<div class="pinned-container hidden">
</div>
