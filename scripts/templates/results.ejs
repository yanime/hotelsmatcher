    <li class="photos"><span>Photos</span></li>
    <li class="facilities category title"><span>Facilities</span></li>
    <li class="wifi option checkbox unchecked <%=(obj.facilities.wifi == true)? ' active' : '' %>">
        <input type="hidden" id="wifi" value="<%=(obj.facilities.wifi == true)%>" /><div class="check"></div><span>Wifi</span></li>
    <li class="breakfast option checkbox unchecked <%=(obj.facilities.breakfast == true) ? ' active' : '' %>">
